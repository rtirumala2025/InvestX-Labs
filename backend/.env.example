# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key

# OpenRouter API Configuration (Get key from https://openrouter.ai/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# App Configuration
PORT=5001
NODE_ENV=development

# Frontend URL for CORS and HTTP-Referer header
FRONTEND_URL=http://localhost:3000
APP_URL=http://localhost:3000

# JWT Configuration (if needed for Supabase JWT verification)
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=24h

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,https://your-production-domain.com

# Logging
LOG_LEVEL=debug

# Optional: Database Connection Pool
DB_POOL_SIZE=20
DB_CONNECTION_TIMEOUT=30000
